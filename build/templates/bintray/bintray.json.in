{
  "package": {
    "name": "dogen",
    "repo": "main",
    "subject": "masd-project",
    "desc": "The MASD Code Generator.",
    "website_url": "https://github.com/MASD-Project/dogen",
    "issue_tracker_url": "https://github.com/MASD-Project/dogen/issues",
    "vcs_url": "https://github.com/MASD-Project/dogen",
    "github_use_tag_release_notes": true,
    "licenses": [
      "GPL-3.0"
    ],
    "labels": [
      "MDE, MDD, C++"
    ],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "${DOGEN_VERSION}",
    "desc": "Sprint ${DOGEN_PATCH_VERSION}",
    "released": "${BUILD_DATE}",
    "vcs_tag": "v${DOGEN_VERSION}",
    "gpgSign": false
  },
  "files": [
    {
      "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-applications.deb",
      "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-applications.deb",
      "matrixParams": {
        "override": 1
      }
    },
    {
      "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-headers.deb",
      "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-headers.deb",
      "matrixParams": {
        "override": 1
      }
    },
    {
      "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-libraries.deb",
      "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-libraries.deb",
      "matrixParams": {
        "override": 1
      }
    },
    {
      "includePattern": "${DOGEN_PACKAGE_DIR}/dogen_${DOGEN_VERSION}_amd64-tests.deb",
      "uploadPattern": "/${DOGEN_VERSION}/dogen_${DOGEN_VERSION}_amd64-tests.deb",
      "matrixParams": {
        "override": 1
      }
    },
    {
      "includePattern": "${DOGEN_PACKAGE_DIR}/DOGEN-${DOGEN_VERSION}-Darwin-x86_64.dmg",
      "uploadPattern": "/${DOGEN_VERSION}/DOGEN-${DOGEN_VERSION}-Darwin-x86_64.dmg",
      "matrixParams": {
        "override": 1
      }
    }
  ],
  "publish": true
}
